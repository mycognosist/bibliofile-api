<!-- 'base.html.tera' -->
<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
  </head>
  <body>
    <div class="container">
      <h1>Users</h1>
      {% for user in users %}
        <p>{{ user.username }} - {{ user.email }}</p>
      {% endfor %}

      <h1>Books</h1>
      {% for book in books %}
        <p>{{ book.title }} - {{ book.author }}</p>
      {% endfor %}
    </div>
  </body>
</html>
